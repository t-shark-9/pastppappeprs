{
  "name": "tooessay-desktop",
  "productName": "TooEssay",
  "private": true,
  "version": "1.0.0",
  "description": "TooEssay - AI Writing Coach for IB, A-Levels & Academic Success",
  "author": "TooEssay",
  "license": "MIT",
  "type": "module",
  "main": "electron/dist/main.cjs",
  "scripts": {
    "dev": "vite --port 5173",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "lint": "eslint .",
    "preview": "vite preview",
    "scan-papers": "node scripts/generatePapersIndex.js",
    "electron:compile": "tsc -p electron/tsconfig.json && mv electron/dist/main.js electron/dist/main.cjs && mv electron/dist/preload.js electron/dist/preload.cjs",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npm run electron:compile && electron .\"",
    "electron:preview": "npm run build && npm run electron:compile && electron .",
    "electron:build": "npm run build && npm run electron:compile && electron-builder --win",
    "electron:build:all": "npm run build && npm run electron:compile && electron-builder --win --mac --linux"
  },
